        "test": "ava",
        "prepare": "npm test && npm run types"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/markdown-inline.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/markdown-inline/issues"
    },
    "homepage": "https://github.com/UpperCod/markdown-inline#readme",
    "devDependencies": {
        "ava": "^3.10.1",
        "typescript": "^4.2.4"
    },
    "ava": {
        "files": [
            "tests/**/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    },
    "dependencies": {
        "@uppercod/indentation": "latest"
    }
